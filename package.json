{
  "name": "dicomweb-proxy",
  "version": "1.7.0",
  "description": "A proxy to translate between dicomweb and dimse",
  "bin": "./build/app.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "format": "npx prettier ./src",
    "build": "tsc --p ./src/tsconfig.json",
    "pkg:win": "npm run build && npx pkg --targets node16-win-x64 package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knopkem/dicomweb-proxy.git"
  },
  "keywords": [
    "DICOMWEB",
    "PROXY",
    "DIMSE",
    "DICOM"
  ],
  "author": "Michael Knopke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knopkem/dicomweb-proxy/issues"
  },
  "homepage": "https://github.com/knopkem/dicomweb-proxy#readme",
  "dependencies": {
    "@fastify/autoload": "^5.1.0",
    "@fastify/compress": "^6.1.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/sensible": "^5.1.0",
    "@fastify/static": "^6.4.1",
    "@iwharris/dicom-data-dictionary": "^1.26.0",
    "config": "^3.3.7",
    "deepmerge": "^4.2.2",
    "dicom-dimse-native": "^2.4.0",
    "dicom-parser": "^1.8.13",
    "fastify": "^4.2.1",
    "shelljs": "^0.8.5",
    "simple-node-logger": "^21.8.12",
    "socket.io-client": "^4.5.1",
    "throat": "^6.0.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/eslint": "^8.4.5",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^18.0.5",
    "@types/prettier": "^2.6.3",
    "@types/shelljs": "^0.8.11",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "pkg": {
    "assets": [
      "./config/default.json",
      "./node_modules/dicom-dimse-native/build/Release/dcmtk.node"
    ]
  }
}
